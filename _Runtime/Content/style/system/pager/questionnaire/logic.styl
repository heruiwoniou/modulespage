logic()
	.logic
		display none;
		padding 0 10px
		h1
			line-height 30px
			.inline-container
				float right
				font-size 0
			a
				margin 0 5px
			i
				vertical-align middle
				font-size 12px
				padding 0 5px
		table
			table-layout fixed
			width 100%
			tr
			td
				line-height 30px
			select
				width 100%
				height 25px;
			.label-1
			.label-2
			.label-3
			.label-4
				text-align center
			.label-1
				width 40px
			.label-2
				width 52px
			.label-3
				width 67px
			.label-4
				width 20px;
				a
					text-decoration none;
					font-size 16px;
					margin-left 5px;
					iconfont();
					color $fontSmallColor;
				&:hover
					a
						color $Color7
			.addrow
				td
					line-height 20px;
					a
						text-decoration none;
						font-size 16px;
						margin-left 5px;
						iconfont();
					&:hover
						a
							color $Color3