html
body
  height 100%
  background transparent
  position relative
  .main
    position absolute
    height 100%
    left 0
    right 0
  .data-center
    margin-top 85px
    width 1360px
    .center-nav
      width 100%
      height 50px
      background #ede3ea
      margin-top 72px
      margin-bottom 10px
      overflow hidden
      li
        float left
        width 11.11%
        height 50px
        border-right 1px solid #502659
        a
          display block
          height: 50px
          text-align center
          line-height 50px
          color #502659
          font-size 15px
      li.last
        border-right none!important

      li.active
        a
          color #fff!important
          background #84528a!important




.center-table
  width 446px
  margin-right 10px
  margin-bottom 10px
  background white
  overflow hidden
  float left
  .center-title
    height 60px
    line-height 60px
    padding 0 17px
    color #2b2626
    font-size 16px
    overflow hidden
    span.title-company
      color #999999
      font-size 12px
      float right
  .table-img
    width:100%
    background #f6f3f6
    overflow hidden
    padding-bottom 20px
    img
      width 90%
      //max-height:190px
      margin 15px auto 0
      display block





.bottomIcon
  margin-top 15px
  overflow hidden
  span
    display inline-block
    float right
    padding-left 15px
    margin-right 15px
  span:before
    width: 15px
    height 15px
    content:" "
    float left
    display inline-block
  .yellowIcon:before
    background url("../icons/small-yellow-bac.png") no-repeat;
  .redIcon:before
    background url("../icons/small-red-bac.png") no-repeat;
  .greenIcon:before
    background url("../icons/small-green-bac.png") no-repeat;
  .blueIcon:before
    background url("../icons/small-blue-bac.png") no-repeat;



html
body
  .selfNumber
    width 1370px
