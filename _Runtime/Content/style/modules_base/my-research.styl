@import 'DF-common.styl';
@import './data-center.styl';
@import './Personnel-information.styl';

html
body
  height 100%
  background transparent
  position relative
  .main
    position absolute
    height 100%
    left 0
    right 0
  .tableList
    width 374px
    height auto
    float left
    margin-right 10px
    margin-bottom 20px
    overflow hidden
    .tableTitle
      height 30px
      width 100%
      padding 0 10px 0 17px
      font-size 14px
      color #2b2626
      line-height 30px
      background white
      span
        float right
        font-size 12px
        color #999999



