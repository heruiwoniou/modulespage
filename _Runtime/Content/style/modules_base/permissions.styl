@import './../setting.styl';
@import './../base/core/common.styl';
.content-title
  height 60px
  line-height 60px
  border-top 2px #ffb033 solid
  text-indent 10px


.right-btn
  float right
  margin-right 30px
.content-list-box
  width 100%
  height auto
  overflow hidden
  padding-bottom 30px

.list
  width 96px
  height 136px
  float left
  position relative
  box-shadow 0 0 0 0 #c9ede1;
  margin 20px 20px 0 30px
  .pic-icon1
    width 100%
    height 96px
    line-height 96px
    text-align center
    font-size 42px
    color white
    margin 0
    border-radius 3px
    position absolute
    left 0
    top 0
    cursor pointer
  .pic-icon2
    width 100%
    height 96px
    line-height 96px
    text-align center
    font-size 16px
    color white
    margin 0
    background black
    background rgba(0,0,0,0.2)
    border-radius 3px
    position absolute
    left 0
    top 0
    visibility  hidden
.list:hover .pic-icon2
    visibility  visible
  h5
    width 100%
    height 40px
    line-height 40px
    text-align center
    position absolute
    left 0
    bottom 0
