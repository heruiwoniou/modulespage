.comsys-ProtoUpload
	overflow hidden
	background #c1c1c1
	border 1px solid $border-color
	height 100px
	width 100px
	position relative
	box-sizing:border-box
	border-radius $borderradius
	transition box-shadow .1s ease , border .1s ease
	line-height 100px
	font-size 0px
	text-align center
	img
		vertical-align middle
		margin 0 auto
	&:after
		iconfont()
		content ''
		vertical-align middle
		display inline-block
		font-size 55px
		position relative
		top -4px
		color white
	&.focus-outerline
		outerlineset($inputoutlinecolor)
.comsys-ProtoUpload
	input[type=file]
		height: 100%
		position: absolute
		left: 0px
		top: 0px
		width: 100%
		opacity 0
		z-index 1
		cursor pointer
	div
		margin-right: 30px
		height: 100%
		text-indent:5px
		textHidden()
	span
		position: absolute
		right: 0px
		text-align: center
		width:30px
		line-height: 100%
		height: 100%
		top: 0px
		font-size:$font-ico-size
		.textHidden()
		&:before
			font-family: "ElegantIcons"
			line-height: 1.5
			content:""