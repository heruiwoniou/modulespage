.progress
	background: rgb(235, 235, 235); border-radius: 10px; height: 20px; border-right-color: transparent; border-left-color: transparent; border-right-width: 1px; border-left-width: 1px; border-right-style: solid; border-left-style: solid;
	&>span
		background: rgb(204, 204, 204); border-width: 1px; border-style: solid; border-color: rgb(191, 191, 191) rgb(179, 179, 179) rgb(158, 158, 158); margin: 0px -1px; border-radius: 10px; height: 18px; text-align: right; line-height: 16px; float: left; position: relative; min-width: 30px; box-shadow: inset 0px 1px rgba(255,255,255,0.3), 0px 1px 2px rgba(0,0,0,0.2); -webkit-box-shadow: inset 0 1px rgba(255, 255, 255, 0.3), 0 1px 2px rgba(0, 0, 0, 0.2);transition width .2s;
		&:before
			/* @meta {"spritesheet": {"include": false}} */
			background: url($imagepath + "progress.png") repeat-x 0px 0px;
			border-radius: 10px; left: 0px; top: 0px; height: 18px; right: 0px; bottom: 0px; position: absolute; z-index: 1; content: "";
		&>span
			padding: 0px 8px; color: rgba(0, 0, 0, 0.7); font-size: 11px; font-weight: bold; text-shadow: 0 1px rgba(255, 255, 255, 0.4);
	.green
		background: rgb(133, 196, 64); border-color: rgb(120, 179, 55) rgb(107, 160, 49) rgb(86, 129, 40);
	.red
		background: rgb(219, 58, 39); border-color: rgb(199, 51, 33) rgb(177, 45, 30) rgb(142, 36, 24);
	.orange
		background: rgb(242, 182, 60); border-color: rgb(240, 173, 36) rgb(235, 163, 16) rgb(197, 136, 13);
	.blue
		background: rgb(90, 170, 219); border-color: rgb(69, 159, 214) rgb(48, 148, 210) rgb(39, 125, 178);