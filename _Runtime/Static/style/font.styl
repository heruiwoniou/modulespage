@font-face
	font-family: 'ElegantIcons';
	src:  url('./../fonts/ElegantIcons.eot?lav3mv');
	src:  url('./../fonts/ElegantIcons.eot?lav3mv#iefix') format('embedded-opentype'),
		url('./../fonts/ElegantIcons.ttf?lav3mv') format('truetype'),
		url('./../fonts/ElegantIcons.woff?lav3mv') format('woff'),
		url('./../fonts/ElegantIcons.svg?lav3mv#ElegantIcons') format('svg');
	font-weight: normal;
	font-style: normal;
@font-face
	font-family: 'icomoon';
	src:  url('./../fonts/icomoon.eot?lav3mv');
	src:  url('./../fonts/icomoon.eot?lav3mv#iefix') format('embedded-opentype'),
		url('./../fonts/icomoon.ttf?lav3mv') format('truetype'),
		url('./../fonts/icomoon.woff?lav3mv') format('woff'),
		url('./../fonts/icomoon.svg?lav3mv#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;

setFontDefault()
	font-family: 'ElegantIcons' !important;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
setFontSystem()
	font-family: 'icomoon' !important;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
[class^="icon-"], [class*=" icon-"]
	setFontSystem()


