c()
	&:after
		display: block
		visibility: hidden
		content:""
		width: 0px
		height:0px
		line-height: 0px
		font-size:0px
		clear:both
		zoom:1
cf()
	overflow auto
	zoom 1
th()
	overflow hidden
	text-overflow ellipsis
	white-space nowrap
fr()
	float right
fl()
	float left
ib()
	display inline-block
	vertical-align middle
